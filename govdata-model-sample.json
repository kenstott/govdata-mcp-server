{
  "version": "1.0",
  "defaultSchema": "econ",
  "schemas": [
    {
      "name": "econ",
      "type": "custom",
      "factory": "org.apache.calcite.adapter.govdata.GovDataSchemaFactory",
      "operand": {
        "dataSource": "econ",
        "refreshInterval": "1h",
        "executionEngine": "DUCKDB",
        "database_filename": "shared.duckdb",
        "ephemeralCache": false,
        "cacheDirectory": "${GOVDATA_CACHE_DIR:govdata-cache}",
        "directory": "${GOVDATA_PARQUET_DIR:govdata-parquet}",
        "startYear": 2023,
        "endYear": 2024,
        "autoDownload": true,
        "enabledSources": ["fred"],
        "fredApiKey": "${FRED_API_KEY}",
        "customFredSeries": [
          "DGS10",
          "UNRATE",
          "CPIAUCSL"
        ]
      }
    },
    {
      "name": "sec",
      "type": "custom",
      "factory": "org.apache.calcite.adapter.govdata.GovDataSchemaFactory",
      "operand": {
        "dataSource": "sec",
        "refreshInterval": "1h",
        "executionEngine": "DUCKDB",
        "database_filename": "shared.duckdb",
        "ephemeralCache": false,
        "cacheDirectory": "${GOVDATA_CACHE_DIR:govdata-cache}",
        "directory": "${GOVDATA_PARQUET_DIR:govdata-parquet}",
        "startYear": 2023,
        "endYear": 2024,
        "autoDownload": true,
        "ciks": ["0000320193"],
        "filingTypes": ["10-K", "10-Q"],
        "fetchStockPrices": false,
        "extractFootnotes": false,
        "extractMDA": false,
        "extractRiskFactors": false,
        "textSimilarity": {
          "enabled": false
        }
      }
    }
  ]
}
